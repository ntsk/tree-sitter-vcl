{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tree_sitter_vcl",
      "camelcase": "TreeSitterVcl",
      "title": "TreeSitterVcl",
      "scope": "source.vcl",
      "file-types": [
        "vcl"
      ],
      "injection-regex": "^vcl$",
      "class-name": "TreeSitterVcl"
    }
  ],
  "metadata": {
    "version": "0.2.1",
    "license": "MIT",
    "description": "Varnish Configuration Language grammar for tree-sitter",
    "authors": [
      {
        "name": "ntsk",
        "email": "ntsk@ntsk.jp",
        "url": "https://ntsk.jp/"
      }
    ],
    "links": {
      "repository": "https://github.com/ntsk/tree-sitter-vcl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}